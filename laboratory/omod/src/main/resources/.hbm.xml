<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.laboratory.model">

	<class name="LabDoctor" table="lab_doctor_details" batch-size="500">

		<id name="id" type="int" column="id"
			unsaved-value="0">
				<generator class="native"/>
		</id>
		<property name="doctorName" column="doc_name" />
                <property name="doctorDegree" column="doc_degree" />
                <property name="doctorDesignation" column="designation" />
                <property name="doctorWorkplace" column="workplace" />
                <property name="creator" column="creator" />
                <property name="createdDate" column="create_date" />
                 	 
	</class>
</hibernate-mapping>
