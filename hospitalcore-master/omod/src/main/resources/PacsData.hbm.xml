<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.hospitalcore.model">

	<class name="PacsData" table="pacs_data" batch-size="500">

		<id name="id" type="int" column="id"
			unsaved-value="0">
			<generator class="native" />
		</id>
		
                
		<property name="accessionNumber" column="accession_number" not-null="true" />
                <property name="study" column="study" />
                <property name="studyDate" column="study_date" />
                <property name="studyTime" column="study_time" />
                <property name="recevingDate" column="receving_date" />
                <property name="reportDate" column="report_date" />
                <property name="readingPhysician" column="reading_physician" />
                <property name="readingPhysicianId" column="reading_physician_id" />
                <property name="reportData" column="report_data" />
                <property name="studyStatus" column="study_status" />
                
                
	</class>
</hibernate-mapping>
