<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.registration.model">

	<class name="DiaPatientEdit" table="patient" batch-size="500">

		<id name="patientId" type="int" column="patient_id"
			unsaved-value="0">
				<generator class="native"/>
		</id>
		<property name="changedBy" column="changed_by" />
		<property name="dateChanged" column="date_changed" /> 
		 
	</class>
</hibernate-mapping>
