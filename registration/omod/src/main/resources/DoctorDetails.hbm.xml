<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.registration.model">

	<class name="DocDetails" table="dia_doctor_details" batch-size="500">

		<id name="id" type="int" column="id"
			unsaved-value="0">
				<generator class="native"/>
		</id>
		<property name="doctorName" column="doctor_name" />
		<property name="creator" column="creator" /> 
		<property name="createdDate" column="created_on" />
                <property name="designation" column="designation" />
                <property name="address" column="address" />
                <property name="phone" column="phone" />
                <property name="degree" column="degree" />
                <property name="marketed_by" column="marketed_by" />
                
	</class>
</hibernate-mapping>
